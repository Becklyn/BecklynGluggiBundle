{
    "name": "becklyn/gluggi-bundle",
    "type": "symfony-bundle",
    "description": "Modular layout preview system, to be used within a symfony app.",
    "homepage": "https://github.com/Becklyn/GluggiBundle",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "becklyn/assets-bundle": "^2.3",
        "symfony/dependency-injection": "^4.1",
        "symfony/finder": "^4.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/http-kernel": "^4.1",
        "symfony/translation": "^4.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/yaml": "^4.1",
        "twig/twig": "^2.10"

    },
    "require-dev": {
        "becklyn/php-cs": "^1.5",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "^4.1",
        "symfony/css-selector": "^4.1",
        "symfony/phpunit-bridge": "^4.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/var-dumper": "^4.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\GluggiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\GluggiBundle\\": "tests/"
        }
    }
}
